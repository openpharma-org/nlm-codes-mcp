{
  "name": "@uh-joan/codes-mcp-server",
  "version": "0.1.2",
  "description": "MCP server for clinical table search services - provides access to clinical coding systems and medical data tables",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "codes-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "npm test -- --testPathPattern=tests/unit --testPathIgnorePatterns=tests/integration",
    "test:integration": "npm test -- --testPathPattern=tests/integration --testPathIgnorePatterns=tests/unit",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:html": "npm test -- --coverage --coverageReporters=html",
    "test:coverage:lcov": "npm test -- --coverage --coverageReporters=lcov",
    "test:coverage:json": "npm test -- --coverage --coverageReporters=json",
    "test:coverage:text": "npm test -- --coverage --coverageReporters=text-summary",
    "test:coverage:badges": "npm run test:coverage:json && node scripts/coverage-badge.js",
    "test:client": "node tests/integration/test-client.js",
    "lint": "eslint src tests --ext .ts,.js",
    "lint:fix": "eslint src tests --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "bench": "node scripts/benchmark.js",
    "bench:tools": "node scripts/benchmark-tools.js",
    "bench:transports": "node scripts/benchmark-transports.js",
    "api:test": "node scripts/api-integration-test.js",
    "perf": "node scripts/performance-monitor.js",
    "perf:enhanced": "node scripts/performance-monitor.js --enhanced",
    "dashboard": "node scripts/dashboard.js",
    "report": "node scripts/dashboard.js --markdown",
    "ci": "npm run build && npm run lint && npm run type-check && npm run test:coverage",
    "ci:quick": "npm run build && npm run test",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "quality:check": "npm run lint && npm run type-check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "prepublish:check": "node scripts/pre-publish-check.js",
    "package:validate": "npm run clean && npm run build && npm run ci && npm run security:audit",
    "package:test": "npm pack --dry-run && npm run package:install-test",
    "package:install-test": "npm pack && npm install -g ./$(npm pack --silent) && codes-mcp-server --help && npm uninstall -g $(node -p 'require(\"./package.json\").name')",
    "publish:safe": "npm run prepublish:check && npm publish",
    "publish:beta": "npm run prepublish:check && npm publish --tag beta",
    "publish:dry": "npm run prepublish:check && npm publish --dry-run",
    "release:patch": "npm run prepublish:check && npm version patch && git push && git push --tags",
    "release:minor": "npm run prepublish:check && npm version minor && git push && git push --tags",
    "release:major": "npm run prepublish:check && npm version major && git push && git push --tags",
    "health:check": "node -e \"console.log('Health check passed'); process.exit(0)\"",
    "prepublishOnly": "npm run package:validate",
    "preversion": "npm run prepublish:check",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "clinical",
    "medical",
    "coding",
    "icd",
    "loinc",
    "snomed",
    "healthcare",
    "terminology",
    "fhir",
    "ai",
    "tools"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uh-joan/codes-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/uh-joan/codes-mcp-server/issues"
  },
  "homepage": "https://github.com/uh-joan/codes-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/js": "^9.29.0",
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "benchmark": "^2.1.4",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "microtime": "^3.1.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
